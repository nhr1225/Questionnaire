<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="makeform">

  <select id="select" parameterType="String" resultType="com.java.nhr1225.bean.makeformBean">
 		select * from makeform where id = ${id}
  </select>
  <insert id="insert" parameterType="hashmap">
 		insert into makeform (id, title, subtitle, option) values (#{id}, #{title}, #{subtitle},#{option})
  </insert>

</mapper>
